{
  "name": "@matt-dunn/react-wireframes",
  "version": "2.1.1",
  "author": "Matt Dunn",
  "homepage": "https://matt-dunn.github.io/",
  "description": "",
  "keywords": [],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx,.mdx --fix",
    "test": "jest --coverage --passWithNoTests",
    "build:types": "tsc -b tsconfig.build.json",
    "build:js": "babel ./src -d lib --extensions \".ts,.tsx\" --ignore **/*.story.ts,**/*.story.tsx,**/*.spec.ts,**/*.spec.tsx --root-mode upward",
    "prebuild": "npm run lint",
    "build": "npm run build:js && npm run build:types"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "scroll-into-view-if-needed": "^2.2.25"
  },
  "peerDependencies": {
    "@emotion/core": "10.*",
    "@emotion/css": "10.*",
    "@emotion/styled": "10.*",
    "react-transition-group": "4.*"
  }
}
