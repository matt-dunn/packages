{
  "name": "@matt-dunn/state-mutate-with-status",
  "version": "1.0.13",
  "author": "Matt Dunn",
  "homepage": "https://github.com/matt-dunn/packages/tree/master/packages/state-mutate-with-status",
  "description": "State mutator with decorated status for async actions",
  "keywords": [
    "state",
    "async",
    "actions",
    "redux",
    "flux",
    "reducer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-dunn/packages.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:docs": "typedoc --out ../../dist/packages/state-mutate-with-status/docs src --mode file --exclude \"**/*+(.spec|.e2e).*\" --excludeNotExported",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx,.mdx --fix",
    "test": "jest --coverage --passWithNoTests",
    "build:types": "tsc -b tsconfig.build.json",
    "build:js": "babel ./src -d lib --extensions \".ts,.tsx\" --ignore **/*.story.ts,**/*.spec.ts --root-mode upward",
    "prebuild": "npm run lint",
    "build": "npm run build:js && npm run build:types && yarn run build:docs"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.159",
    "@types/react": "^16.9.46"
  },
  "dependencies": {
    "is-promise": "^4.0.0",
    "object-path-immutable": "^4.1.0",
    "simple-deep-freeze": "^0.0.2"
  },
  "peerDependencies": {
    "lodash": "4.*"
  }
}
